#: src/guess.c:60
#, c-format
msgid "Usage: guess [OPTIONS]\n"
msgstr "Использование: guess [ОПЦИИ]\n"

#: src/guess.c:61
#, c-format
msgid ""
"Guess the number game.\n"
"\n"
msgstr ""
"Игра «Угадай число».\n"
"\n"

#: src/guess.c:62
#, c-format
msgid "  -r         Use Roman numerals (I-C)\n"
msgstr "  -r         Использовать римские цифры (I-C)\n"

#: src/guess.c:63
#, c-format
msgid "  --help     Display this help and exit\n"
msgstr "  --help     Показать эту справку и выйти\n"

#: src/guess.c:64
#, c-format
msgid "  --version  Output version information and exit\n"
msgstr "  --version  Показать информацию о версии и выйти\n"

#: src/guess.c:95
#, c-format
msgid "Think of a number between 1 and %d.\n"
msgstr "Загадайте число от 1 до %d.\n"

#: src/guess.c:102
#, c-format
msgid "Is your number greater than %s? (y/n): "
msgstr "Ваше число больше %s? (y/n): "

#: src/guess.c:114
#, c-format
msgid "Your number is %s!\n"
msgstr "Ваше число: %s!\n"
